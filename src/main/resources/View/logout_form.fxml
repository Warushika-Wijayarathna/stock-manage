<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootNode" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.stockmanage.Controller.LogoutFormController">
    <children>
        <Label layoutX="20.0" layoutY="27.0" prefHeight="34.0" prefWidth="394.0" text="Are you sure you want to logout?" textFill="#ff2007">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
      <Group layoutX="91.0" layoutY="89.0">
         <children>
            <Group>
               <children>
                    <Button mnemonicParsing="false" onAction="#btnConfirmLogout" prefHeight="43.0" prefWidth="106.0" style="-fx-background-color: #ff7675;" text="Logout" />
               </children>
            </Group>
              <Button layoutX="113.0" mnemonicParsing="false" onAction="#btnCancelLogout" prefHeight="44.0" prefWidth="106.0" style="-fx-background-color: #ff7675;" text="Cancel" />
         </children>
      </Group>
    </children>
</AnchorPane>
